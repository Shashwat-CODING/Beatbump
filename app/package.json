{
	"name": "beatbump-app",
	"version": "2.2.0",
	"scripts": {
		"dev": "npm run sync && vite dev --host --mode dev",
		"cfw:deploy": "npm run build && wrangler publish",
		"cfw:preview:deploy": "wrangler publish --env preview",
		"vercel:deploy": "vercel deploy --prod",
		"netlify:deploy": "netlify deploy --prod",
		"preview": "vite preview --host",
		"build": "vite build --mode production",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"sync": "svelte-kit sync",
		"validate": "npm run format && npm run lint ",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "npm run lint:prettier && npm run lint:styles && npm run lint:ts",
		"lint:prettier": "prettier --plugin-search-dir . --check . ",
		"lint:styles": "stylelint **/*.{css,mjs,svelte,scss,ts}",
		"lint:ts": " eslint \"src/**/*.{js,ts}\" --ext js,ts --fix "
	},
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@fontsource-variable/commissioner": "^5.0.3",
		"@playwright/test": "^1.35.1",
		"@snuffydev/fast-cli": "^0.0.1",
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"@sveltejs/adapter-cloudflare-workers": "^1.1.4",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-vercel": "^3.0.3",
        "@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.30.4",
		"@swc/core": "^1.3.66",
		"@types/node": "^20.3.1",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"cssnano": "^6.0.1",
		"cssnano-preset-default": "^6.0.1",
		"dashjs": "^4.7.4",
		"dotenv": "^16.3.1",
		"esbuild": "^0.18.7",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte": "^2.30.0",
		"hls.js": "^1.5.7",
		"peerjs": "^1.4.7",
		"postcss": "^8.4.24",
		"postcss-html": "^1.5.0",
		"postcss-load-config": "^4.0.1",
		"postcss-safe-parser": "^6.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.67.0",
		"stylelint": "^15.9.0",
		"stylelint-config-sass-guidelines": "^10.0.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-prettier": "^3.0.0",
		"svelte": "^4.2.0",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.32.2",
		"wrangler": "^3.34.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/commissioner": "^5.0.3",
		"eslint-plugin-unused-imports": "^2.0.0"
	}
}
